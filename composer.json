{
  "name": "itinerisltd/page-as-post-type-archive",
  "type": "package",
  "license": "MIT",
  "description": "Allows you to set a page as the archive of a post type.",
  "authors": [
    {
      "name": "Itineris Limited",
      "email": "dev@itineris.co.uk",
      "homepage": "https://itineris.co.uk/",
      "role": "Company"
    },
    {
      "name": "Dan Lapteacru",
      "email": "danlapteacru@gmail.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "wordpress",
    "sage",
    "roots",
    "archive",
    "custom archive page"
  ],
  "support": {
    "email": "dev@itineris.co.uk",
    "issues": "https://github.com/ItinerisLtd/page-as-post-type-archive/issues",
    "source": "https://github.com/ItinerisLtd/page-as-post-type-archive"
  },
  "autoload": {
    "psr-4": {
      "Itineris\\PageAsPostTypeArchive\\": "src/"
    }
  },
  "require": {
    "php": "^7.3|^8.0"
  },
  "require-dev": {
    "itinerisltd/itineris-wp-coding-standards": "^0.4.0"
  },
  "extra": {
    "acorn": {
      "providers": [
        "Itineris\\PageAsPostTypeArchive\\ArchiveServiceProvider"
      ]
    }
  },
  "scripts": {
    "style:check": "phpcs",
    "style:fix": "phpcbf"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
